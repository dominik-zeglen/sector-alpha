{
  "name": "sector-alpha",
  "version": "1.0.0",
  "description": "Browser-based economic sim set in space",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack serve",
    "build": "NODE_ENV=production webpack",
    "gen-archetype": "hygen archetype new",
    "test": "jest",
    "lint": "npx eslint \"src/**/*.@(tsx|ts|jsx|js)\" --fix ; npx prettier --check \"src/**/*.@(tsx|ts|jsx|js)\" --write"
  },
  "author": "Dominik Żegleń",
  "license": "ISC",
  "devDependencies": {
    "@types/graphlib": "^2.1.8",
    "@types/jest": "^27.0.2",
    "@types/mathjs": "^9.4.2",
    "@types/p5": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "esbuild": "^0.13.6",
    "esbuild-jest": "^0.5.0",
    "esbuild-loader": "^2.16.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "fork-ts-checker-webpack-plugin": "^6.4.2",
    "html-webpack-plugin": "^5.3.2",
    "hygen": "^6.2.0",
    "jest": "^27.3.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-transform-stub": "^2.0.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^8.8.2",
    "string-replace-loader": "^3.1.0",
    "typescript": "^4.5.2",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "testEnvironment": "jsdom",
    "resetMocks": false,
    "transform": {
      "^.+\\.(jsx?|tsx?)$": [
        "esbuild-jest",
        {
          "sourcemap": true
        }
      ],
      ".+\\.(svg|css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "testRegex": ".*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "@pixi-essentials/svg": "^1.1.5",
    "@pixi/graphics": "^6.3.0",
