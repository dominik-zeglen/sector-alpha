{
  "name": "sector-alpha",
  "version": "0.0.1",
  "description": "Browser-based economic sim set in space",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack serve",
    "build": "NODE_ENV=production webpack",
    "gen-archetype": "hygen archetype new",
    "test": "jest",
    "lint": "npx eslint \"@(core|ui|kit|devtools)/**/*.@(tsx|ts|jsx|js)\" --fix ; npx prettier --check \"@(core|ui|kit|devtools)/**/*.@(tsx|ts|jsx|js)\" --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "author": "Dominik Żegleń",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^7.0.0-beta.45",
    "@storybook/addon-essentials": "^7.0.0-beta.45",
    "@storybook/addon-interactions": "^7.0.0-beta.45",
    "@storybook/addon-links": "^7.0.0-beta.45",
    "@storybook/react": "^7.0.0-beta.45",
    "@storybook/react-webpack5": "^7.0.0-beta.45",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/graphlib": "^2.1.8",
    "@types/jest": "^27.0.2",
    "@types/p5": "^1.4.1",
    "@types/react-modal": "^3.13.1",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "^8.3.0",
    "css-loader": "^6.7.1",
    "dts-css-modules-loader": "^2.0.1",
    "esbuild": "^0.13.15",
    "esbuild-jest": "^0.5.0",
    "esbuild-loader": "^2.16.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-storybook": "^0.6.10",
    "fork-ts-checker-webpack-plugin": "^6.4.2",
    "html-webpack-plugin": "^5.3.2",
    "hygen": "^6.2.0",
    "jest": "^27.3.0",
    "jest-canvas-mock": "^2.4.0",
    "jest-transform-stub": "^2.0.0",
    "node-sass": "^7.0.3",
    "prettier": "^2.6.2",
    "prettier-eslint": "^8.8.2",
    "sass-loader": "^13.1.0",
    "storybook": "^7.0.0-beta.45",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^3.3.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.5.2",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "testEnvironment": "jsdom",
    "resetMocks": false,
    "transform": {
      "^.+\\.(jsx?|tsx?)$": [
        "esbuild-jest",
        {
          "sourcemap": true
        }
      ],
      ".+\\.(svg|css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "testRegex": ".*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "@assets/(.*)$": "<rootDir>/assets/$1",
      "@core/(.*)$": "<rootDir>/core/$1",
      "@devtools/(.*)$": "<rootDir>/devtools/$1",
      "@kit/(.*)$": "<rootDir>/kit/$1",
      "@ui/(.*)$": "<rootDir>/ui/$1"
    }
  },
  "dependencies": {
    "@alenaksu/json-viewer": "^1.0.1",
    "@fxts/core": "^0.9.0",
    "@headlessui/react": "^1.6.6",
    "@pixi-essentials/svg": "^1.1.5",
    "@pixi/graphics": "^6.3.0",
    "@pixi/graphics-extras": "^6.3.0",
    "@timohausmann/quadtree-js": "^1.2.5",
    "@types/color": "^3.0.2",
    "@types/lodash": "^4.14.177",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "class-transformer": "^0.5.1",
    "clsx": "^1.1.1",
    "color": "^4.0.1",
    "colormap": "^2.3.2",
    "eventemitter3": "^4.0.7",
    "graphlib": "^2.1.8",
    "idb": "^7.0.1",
    "lightweight-charts": "^3.8.0",
    "lodash": "^4.17.21",
    "mathjs": "^10.6.0",
    "npm": "^8.7.0",
    "p5": "^1.4.1",
    "pixi-viewport": "^4.34.4",
    "pixi.js": "^6.3.0",
    "react": "^17.0.2",
    "react-click-away-listener": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.32.2",
    "react-inlinesvg": "^3.0.0",
    "react-modal": "^3.15.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.5",
    "reflect-metadata": "^0.1.13"
  }
}
